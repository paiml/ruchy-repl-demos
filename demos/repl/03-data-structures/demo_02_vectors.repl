// Dynamic vectors in Ruchy
let mut vec = vec![1, 2, 3]
vec.push(4)
vec.push(5)
println(vec)

// Vector operations
let popped = vec.pop()
println(popped)
println(vec)

// Vector capacity and length
println("Length: {}", vec.len())
println("Capacity: {}", vec.capacity())

// Vector iteration
for item in vec {
    println(item * 2)
}