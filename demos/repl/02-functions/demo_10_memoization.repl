// Memoization example
use std::collections::HashMap;

fun memoize_fib() -> FnMut(u32) -> u32 {
    let cache = new();
    move |n| {
        if let Some(&result) = cache.get(&n) {
            return result;
        }
        let result = if n <= 1 {
            n
        } else {
            fib(n - 1) + fib(n - 2)
        };
        cache.insert(n, result);
        result
    }
}

let fib = memoize_fib();
fib(10)
fib(20)