// Function chaining and pipelines
fun add_one(x) { x + 1 }
fun double(x) { x * 2 }
fun square(x) { x * x }

// Chain functions
let result = square(double(add_one(3)))
result

// Using method chaining
3.add_one().double().square()

// Pipeline operator simulation
let pipeline = |x| square(double(add_one(x)))
pipeline(5)